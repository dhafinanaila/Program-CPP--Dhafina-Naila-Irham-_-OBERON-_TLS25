#include <iostream>
#include <string>
using namespace std;

bool isDigit(char c){ return c >= '0' && c <= '9'; }

void reverseStr(string &s) {
    int i = 0, j = (int)s.size() - 1;
    while (i < j) {
        char t = s[i];
        s[i] = s[j];
        s[j] = t;
        i++; j--;
    }
}

int main() {
    string pwd;
    cin >> pwd;

    int n = (int)pwd.size();
    int L = -1, R = -1; 
    for (int i = 0; i < n; i++) {
        if (isDigit(pwd[i])) {
            if (L == -1) L = i;
            R = i;
        }
    }

    if (L == -1) {
        cout << "Tidak ada angka ASCII di sandi.\n";
        return 0;
    }

    string digitStr = pwd.substr(L, R - L + 1);
    int asciiVal = 0;

    for (char c : digitStr) asciiVal = asciiVal * 10 + (c - '0');
    char firstChar = (char)asciiVal;


    string revCons = pwd.substr(0, L) + pwd.substr(R + 1);
    reverseStr(revCons);

    cout << "ASCII: " << asciiVal << "\n";
    cout << "Huruf pertama asli: " << firstChar << "\n";
    cout << "Konsonan (urutan asli, vokal hilang): " << revCons << "\n";

    string candidate = string(1, firstChar) + revCons;
    cout << "Rekonstruksi parsial: " << candidate << "\n";

    return 0;
}
